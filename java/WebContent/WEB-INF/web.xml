<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>channel</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Opened</servlet-name>
    <servlet-class>game.Opened</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Opened</servlet-name>
    <url-pattern>/opened</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Move</servlet-name>
    <servlet-class>game.Move</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Move</servlet-name>
    <url-pattern>/move</url-pattern>
  </servlet-mapping>
</web-app>